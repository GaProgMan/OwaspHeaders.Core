{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "OwaspHeaders.Core": "Debug"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "OwaspHeaders.Core": "Debug"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": false,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss.fff "
      }
    }
  }
}
